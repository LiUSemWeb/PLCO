    <owl:ObjectProperty rdf:about="http://w3id.org/PLCO/ontology/processODP#hasOutput">
        <rdfs:domain rdf:resource="http://w3id.org/PLCO/ontology/processODP#Process"/>
        <rdfs:comment>The output of a process or event.</rdfs:comment>
        <rdfs:label>has output</rdfs:label>
    </owl:ObjectProperty>
    


