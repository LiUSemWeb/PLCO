            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/PLCO/ontology/processODP#isSettingFor"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/PLCO/ontology/processODP#Process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/PLCO/ontology/processODP#precedesExecution"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/PLCO/ontology/processODP#Situation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/PLCO/ontology/processODP#satisfiesPlan"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/PLCO/ontology/processODP#Plan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Plan executions are situations that proactively satisfy a plan. Subplan executions are proper parts of the whole plan execution.</rdfs:comment>
        <rdfs:label>Plan Execution</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/PLCO/ontology/processODP#Process -->


    <owl:Class rdf:about="http://w3id.org/PLCO/ontology/processODP#Process">
        <rdfs:subClassOf>
