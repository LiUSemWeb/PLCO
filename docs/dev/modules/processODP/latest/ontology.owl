    <!-- http://w3id.org/PLCO/ontology/processODP#duringTime -->


    <owl:ObjectProperty rdf:about="http://w3id.org/PLCO/ontology/processODP#duringTime">
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Interval"/>
        <rdfs:comment>The time interval during which something, e.g. a process or event, takes place.</rdfs:comment>
        <rdfs:label>during time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/PLCO/ontology/processODP#followsExecution -->


    <owl:ObjectProperty rdf:about="http://w3id.org/PLCO/ontology/processODP#followsExecution">
