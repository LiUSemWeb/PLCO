        <rdfs:domain rdf:resource="http://w3id.org/PLCO/ontology/processODP#Situation"/>
        <rdfs:comment>The thing(s) that this situation is the setting for, e.g. a resource that is in a certain state.</rdfs:comment>
        <rdfs:label>is the setting for</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/PLCO/ontology/processODP#precedesExecution -->


    <owl:ObjectProperty rdf:about="http://w3id.org/PLCO/ontology/processODP#precedesExecution">
