                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/PLCO/ontology/processODP#followsExecution"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/PLCO/ontology/processODP#Situation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/PLCO/ontology/processODP#hasPart"/>
