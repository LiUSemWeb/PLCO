                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Something that happens within a given context or (short) timeframe.</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/PLCO/ontology/processODP#Plan -->


    <owl:Class rdf:about="http://w3id.org/PLCO/ontology/processODP#Plan">
