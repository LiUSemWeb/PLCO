  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "An agent able to act in the context of a circular value network, e.g. an organisation, person."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
